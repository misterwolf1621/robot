<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>robot control</title>
    <script src="https://code.jquery.com/jquery-3.6.3.min.js"></script>
    <script src="index.js"></script>
    <link rel="stylesheet" href="style.css">
    <style>

    </style>
</head>
<body>
    <table>
        <tr>
            <td class="marked">current speed</td>
            <td></td>
            <td><button id="forward" onclick="direction('np');">+</button> </td>
            <td></td>
            <td class="marked">current speed</td>
        </tr>
        <tr>
            <td id="speedleft" class="marked"></td>
            <td id="lp"><button onclick="direction('lp');">+</button></td>
            <td></td>
            <td id="rp"><button onclick="direction('rp');">+</button></td>
            <td id="speedright" class="marked"></td>
        </tr>
        <tr>
            <td></td>
            <td id="lm"><button onclick="direction('lm');">-</button></td>
            <td></td>
            <td id="rm"><button onclick="direction('rm');">-</button></td>
            <td></td>
        </tr>
        <tr>
            <td></td>
            <td></td>
            <td><button id="backward" onclick="direction('nm');">-</button></td>
            <td></td>
            <td><button id="water" onclick="direction('w');">-</button></td>
        </tr>
    </table>

    <form action="send" method="get">
        <input type="text" name="send">
        <input type="submit">
    </form>
    
    
    
    
    
    
</body>
</html>